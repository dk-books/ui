{
  "name": "@dk-books/ui",
  "homepage": "https://dk-books.github.io/ui/",
  "version": "0.9.01",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "//": "New scripts to run and build Storybook with Tailwind",
    "storybook": "concurrently \"yarn:watch:*\"",
    "build-storybook": "concurrently \"yarn:build:*\"",
    "build:css": "npx tailwindcss -i ./src/tailwind.css -o ./public/tailwind.css",
    "build:storybook": "storybook build",
    "watch:css": "npx tailwindcss -i ./src/tailwind.css -o ./public/tailwind.css --watch",
    "watch:storybook": "storybook dev -p 6006",
    "release": "npm publish",
    "lint": "next lint"
  },
  "dependencies": {
    "@apollo/client": "^3.8.3",
    "@cloudinary/url-gen": "^1.11.2",
    "@graphql-mocks/network-nock": "^0.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.8",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "classnames": "^2.3.2",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^1.0.1",
    "graphql-mocks": "^0.10.0",
    "graphql-request": "^6.1.0",
    "next": "^13.5.2",
    "next-router": "^1.3.6",
    "next-router-mock": "^0.9.9",
    "next-routes": "^1.4.2",
    "nock": "^13.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.46.1",
    "react-icons": "^4.11.0",
    "react-scripts": "5.0.1",
    "tailwindcss": "3.3.3",
    "typescript": "^4.9.4",
    "use-local-storage": "^3.0.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.17",
    "@storybook/addon-actions": "^7.4.1",
    "@storybook/addon-essentials": "^7.0.0-beta.12",
    "@storybook/addon-interactions": "^7.0.0-beta.12",
    "@storybook/addon-links": "^7.0.0-beta.12",
    "@storybook/addon-mdx-gfm": "^7.4.1",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.0.0-beta.12",
    "@storybook/builder-webpack5": "^6.5.14",
    "@storybook/cli": "^7.4.1",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/node-logger": "^7.0.0-beta.12",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^7.0.0-beta.12",
    "@storybook/react-webpack5": "^7.0.0-beta.12",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@types/jest": "^29.5.4",
    "add": "^2.0.6",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "concurrently": "^7.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "jest": "^29.7.0",
    "postcss": "^8.4.20",
    "prop-types": "^15.8.1",
    "sass": "^1.67.0",
    "storybook": "^7.0.0-beta.12",
    "tailwindcss": "^3.2.4",
    "webpack": "^5.75.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
