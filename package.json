{
  "name": "@dk-books/ui",
  "homepage": "https://dk-books.github.io/ui/",
  "version": "17.0.1",
  "main": "build/bundle.js",
  "repository": "dk-books/ui",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "peerDependencies": {
		"next": "^13.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "predeploy": "npm run build-storybook",
    "clean": "react-scripts clean",
    "build": "rollup -c",
    "release": "npm publish",
    "lint": "eslint --ext .tsx,.ts src/components",
    "lint-fix-dry-run": "eslint --ext .tsx,.ts src/components --fix-dry-run",
    "lint-fix": "eslint --ext .tsx,.ts src/components --fix",
    "test": "react-scripts test",
    "test-ci": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s public",
    "serve-storybook": "serve storybook-static",
    "deploy-storybook": "gh-pages -d storybook-static",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@cloudinary/url-gen": "^1.12.1",
    "@graphql-mocks/network-nock": "^0.5.0",
    "@heroicons/react": "^2.0.18",
    "@sls-next/serverless-component": "^3.7.0",
    "@tailwindcss/typography": "^0.5.10",
    "dompurify": "^3.0.6",
    "graphql": "^16.8.1",
    "graphql-mocks": "^0.10.0",
    "graphql-request": "^6.1.0",
    "nock": "^13.3.8",
    "npm-check": "^6.0.1",
    "ra-data-graphql": "^4.14.3",
    "react": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "storybook-addon-designs": "^6.3.1",
    "use-local-storage": "^3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-json": "6.0.1",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@storybook/addon-actions": "^7.5.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/builder-webpack5": "^7.5.3",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^7.5.3",
    "@storybook/preset-create-react-app": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "14.1.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^29.5.8",
    "@types/next": "^9.0.0",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "classnames": "2.3.2",
    "eslint": "^8.53.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^1.0.1",
    "eslint-plugin-react": "^7.33.2",
    "gh-pages": "^6.0.0",
    "husky": "^8.0.3",
    "jest-axe": "^8.0.0",
    "next-router-mock": "^0.9.10",
    "postcss": "^8.4.31",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-scripts": "5.0.1",
    "rollup-plugin-copy": "3.5.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-typescript2": "0.36.0",
    "storybook-addon-next-router": "^4.0.2",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "web-vitals": "^3.5.0",
    "webpack": "^5.89.0"
  }
}
