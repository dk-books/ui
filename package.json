{
  "name": "@dk-books/ui",
  "homepage": "https://dk-books.github.io/ui/",
  "version": "0.3.54",
  "main": "build/bundle.js",
  "repository": "dk-books/ui",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "peerDependencies": {
    "next": "^12.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "predeploy": "npm run build-storybook",
    "clean": "react-scripts clean",
    "build": "rollup -c",
    "release": "npm publish",
    "lint": "eslint --ext .tsx,.ts src/components",
    "lint-fix-dry-run": "eslint --ext .tsx,.ts src/components --fix-dry-run",
    "lint-fix": "eslint --ext .tsx,.ts src/components --fix",
    "test": "react-scripts test",
    "test-ci": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@cloudinary/url-gen": "^1.9.0",
    "@graphql-mocks/network-nock": "^0.5.0",
    "@heroicons/react": "^1.0.6",
    "@sls-next/serverless-component": "^3.7.0",
    "@tailwindcss/typography": "^0.5.9",
    "dompurify": "^2.4.3",
    "graphql": "^16.6.0",
    "graphql-mocks": "^0.10.0",
    "graphql-request": "^5.1.0",
    "nock": "^13.3.0",
    "npm-check": "^6.0.1",
    "ra-data-graphql": "^4.6.0",
    "react-hook-form": "^7.40.0",
    "storybook-addon-designs": "^6.3.1",
    "use-local-storage": "^2.3.6",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "23.0.4",
    "@rollup/plugin-json": "5.0.2",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-webpack5": "^6.5.14",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/node-logger": "^6.5.14",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.14",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/dompurify": "^2.4.0",
    "@types/jest": "^29.2.4",
    "@types/next": "^9.0.0",
    "@types/node": "^18.11.14",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "babel-plugin-named-exports-order": "^0.0.2",
    "classnames": "2.3.1",
    "eslint": "^8.29.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^0.55.1",
    "eslint-plugin-react": "^7.31.11",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.2",
    "jest-axe": "^7.0.0",
    "next-router-mock": "^0.8.0",
    "postcss": "^8.4.20",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-scripts": "5.0.1",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-typescript2": "0.34.1",
    "storybook-addon-next-router": "^4.0.2",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "web-vitals": "^3.1.0",
    "webpack": "^5.75.0"
  }
}
